{
  "name": "gjenny-bs-sak",
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "build:server": "tsc ./app/server/server.ts --outDir dist --target es2022 --module esnext --moduleResolution bundler --allowImportingTsExtensions false",
    "dev": "tsx ./app/server/server.ts",
    "dev:vite": "react-router dev",
    "lint": "eslint . --max-warnings=0",
    "start": "node dist/server.js",
    "start:dev": "npm run build && react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc"
  },
  "dependencies": {
    "@navikt/aksel-icons": "^7.33.3",
    "@navikt/ds-css": "^7.33.3",
    "@navikt/ds-react": "^7.33.3",
    "@react-router/node": "^7.9.6",
    "@react-router/serve": "^7.9.6",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "isbot": "^5.1.32",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@react-router/dev": "^7.9.6",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.5",
    "@types/express-session": "^1.18.2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "jiti": "^2.6.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite": "^7.2.2",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
