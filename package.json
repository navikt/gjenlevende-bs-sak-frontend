{
  "name": "gjenny-bs-sak",
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "build:server": "tsc ./app/server/server.ts --outDir dist --target es2022 --module esnext --moduleResolution bundler --allowImportingTsExtensions false",
    "dev": "tsx ./app/server/server.ts",
    "dev:vite": "react-router dev",
    "start": "node dist/server.js",
    "start:dev": "npm run build && react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc"
  },
  "dependencies": {
    "@navikt/aksel-icons": "^7.33.2",
    "@navikt/ds-css": "^7.33.2",
    "@navikt/ds-react": "^7.33.2",
    "@react-router/node": "^7.9.2",
    "@react-router/serve": "^7.9.5",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "isbot": "^5.1.32",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router": "^7.9.5"
  },
  "devDependencies": {
    "@react-router/dev": "^7.9.5",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.2",
    "@types/node": "^22.18.13",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
